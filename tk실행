import tkinter as tk
window = tk.Tk()
window.title('파이썬')
window.geometry("300x300")

a = tk.Button(window, text = "버튼1")
b = tk.Button(window, text = "버튼2")
c = tk.Button(window, text = "버튼3")
d = tk.Button(window, text = "버튼4")

a.grid(row = 0, column = 0, rowspan = 2)
b.grid(row = 0, column = 1)
c.grid(row = 0, column = 2)
d.grid(row = 1, column = 1, rowspan = 2)

window.mainloop()
